# Environment variables for tunnel-service

# Server port
PORT=5000

# MongoDB Connection URI
MONGODB_URI=mongodb://localhost:27017/tunnel-service

# Stripe API Keys
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Stripe Price IDs (replace with your actual Price IDs from the Stripe Dashboard)
STRIPE_BASIC_PLAN_PRICE_ID=price_...
STRIPE_PRO_PLAN_PRICE_ID=price_...

# Nginx Proxy Manager API
NGINX_PROXY_MANAGER_API_URL=http://nginx-proxy-manager:81/api
NGINX_PROXY_MANAGER_API_KEY=...

# JWT Secret for token signing
JWT_SECRET=your_jwt_secret

# Email for the initial admin user
ADMIN_EMAIL=admin@example.com

# Base domain for tunnels
TUNNEL_DOMAIN=tunnels.yourservice.com

# URL of the frontend app (for Stripe redirects)
FRONTEND_URL=http://localhost:5173

# Qdrant Configuration
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_api_key_or_leave_blank
